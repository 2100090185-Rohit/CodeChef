#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
int t;
cin>>t;
while(t--)
{
       int x,n;
	    cin>>x>>n;
	   int p=n/100;
	    if(n%100==0)
	    {
	        if(p>=x)
	            cout<<p-x<<endl;
	        else
	            cout<<"0"<<endl;
	    }
	    else
	    {
	        p++;
	        if(p>=x)
	            cout<<p-x<<endl;
	        else
	            cout<<"0"<<endl;
	    }
}
}
