#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int X, Y;
        scanf("%d %d", &X, &Y);

        // Calculate the maximum number of months Chef can rent the cooler
        int maxMonths = (Y - 1) / X;

        // Output the result
        printf("%d\n", maxMonths);
    }

    return 0;
}
