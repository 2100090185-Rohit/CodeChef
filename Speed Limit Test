#include <stdio.h>
int main(void) {
	// your code goes here
	int t;
	float a,x,b,y;
	scanf("%d", &t);
	while(t--)
	{
	    scanf("%f %f %f %f", &a, &x, &b, &y);
	    float alice = a/x;
	    float bob = b/y;
	    if(alice == bob)
	    {
	        printf("Equal\n");
	    }
	    else if(alice > bob)
	    {
	        printf("ALice\n");
	    }
	    else
	    {
	        printf("Bob\n");
	    }
	}
	return 0;
}
