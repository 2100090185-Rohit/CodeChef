#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,x;
	std::cin >> t;
	while(t--){
	    std::cin >> x;
	    if(x<=100){
	        std::cout << x << std::endl;
	    }
	    else if(x>100 && x<=1000){
	        std::cout << x-25 << std::endl;
	    }
	    else if(x>1000 && x<=5000){
	        
	        std::cout << x-100 << std::endl;
	    }
	    else if(x>5000){
	        std::cout << x-500 << std::endl;
	    }
	}
	return 0;
}
