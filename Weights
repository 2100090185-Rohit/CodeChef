#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,w,x,z,y;
    std::cin >> t;
    while(t--)
    {
        std::cin >> w>>x>>y>>z;
        string res=(w==x||w==y||w==z||w==x+y||w==y+z||w==x+z|| w==x+y+z)?"Yes":"No";  
        std::cout << res << std::endl;
        
    }
	return 0;
}
