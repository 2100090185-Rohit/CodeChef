#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t,a,b,x,y;
	cin>>t;
	while(t--){
	    cin>>a>>b>>x>>y;
	    if(a==b){
	        cout<<"yes"<<endl;
	    }
	    else if(a>b && (a-y)<=b){
	        cout<<"yes"<<endl;
	    }
	    else if(b>a && (a+x)>=b){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
}
